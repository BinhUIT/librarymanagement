		<parameter name="ReportEndDate" class="java.sql.Date">
			<defaultValueExpression><![CDATA[new java.util.Date()]]></defaultValueExpression>
		</parameter>
		<query language="SQL"><![CDATA[SELECT library.booktype.`NAME` AS `BookTypeName`,
	count(*) AS `BookTypeAmount`
FROM library.borrowingcarddetail
	inner join library.service ON 
	 library.borrowingcarddetail.`SERVICEID` = library.service.`SERVICEID` 
	inner join library.book ON 
	 library.borrowingcarddetail.`BOOKID` = library.book.`ID` 
	inner join library.booktitle ON 
	 library.book.`TITLEID` = library.booktitle.`ID` 
	inner join library.booktype ON 
	 library.booktitle.`TYPEID` = library.booktype.`ID` 
WHERE 
     library.service.`IMPLEMENTDATE` >= $P{ReportStartDate} 
	 AND library.service.`IMPLEMENTDATE` <= $P{ReportEndDate} 
GROUP BY library.booktype.`ID`
ORDER BY library.booktype.`NAME` ASC]]></query>
