		<parameter name="ReportEndDate" class="java.sql.Date">
			<defaultValueExpression><![CDATA[new java.util.Date()]]></defaultValueExpression>
		</parameter>
		<query language="SQL"><![CDATA[SELECT 
    library.booktype.`NAME` AS `BookTypeName`,
    library.booktitle.`NAME` AS `BookTitleName`,
    COUNT(*) AS `BookTitleAmount`
FROM 
    library.borrowingcarddetail
    INNER JOIN library.service 
        ON library.borrowingcarddetail.`SERVICEID` = library.service.`SERVICEID`
    INNER JOIN library.book 
        ON library.borrowingcarddetail.`BOOKID` = library.book.`ID`
    INNER JOIN library.booktitle 
        ON library.book.`TITLEID` = library.booktitle.`ID`
    INNER JOIN library.booktype 
        ON library.booktitle.`TYPEID` = library.booktype.`ID`
WHERE 
	 (($P{BookTypeId} IS NULL) OR (library.booktype.`ID` = $P{BookTypeId}))
	 AND library.service.`IMPLEMENTDATE` >= $P{ReportStartDate} 
	 AND library.service.`IMPLEMENTDATE` <= $P{ReportEndDate} 
	 AND  library.borrowingcarddetail.`Status` IN ('BORROWING', 'RENEWAL', 'RETURNED')
GROUP BY 
    library.booktype.`NAME`,
    library.booktitle.`ID`
ORDER BY 
    COUNT(*) OVER (PARTITION BY library.booktype.`NAME`) DESC,
    library.booktype.`NAME` ASC,
    COUNT(*) DESC,
    library.booktitle.`NAME` ASC;
]]></query>
